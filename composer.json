{
    "name": "kartubi/php-redis-rate",
    "description": "Redis-based rate limiting for PHP/Laravel using GCRA algorithm",
    "keywords": ["laravel", "redis", "rate-limiting", "gcra", "leaky-bucket"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/kartubi/php-redis-rate",
    "authors": [
        {
            "name": "Kartubi",
            "email": "kartubi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/redis": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "laravel/framework": "^10.0|^11.0|^12.0",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kartubi\\RedisRate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kartubi\\RedisRate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kartubi\\RedisRate\\RedisRateServiceProvider"
            ],
            "aliases": {
                "RedisRate": "Kartubi\\RedisRate\\Facades\\RedisRate"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}